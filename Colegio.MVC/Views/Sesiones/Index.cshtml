        <th>
            @Html.DisplayNameFor(model => model.Curso.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Horario.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Profesor.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actividad)
        </th>
        <th>
