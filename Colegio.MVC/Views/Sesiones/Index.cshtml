            @Html.DisplayFor(modelItem => item.Aula.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Curso.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Horario.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Profesor.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Actividad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraFin)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SesionId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SesionId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SesionId })
        </td>
    </tr>
}

</table>
